{
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "json",
    "typescript.tsx",
    "graphql",
    "html",
    "markdown",
    "css",
    "scss",
    "php",
    "ruby",
    "rust"
  ],
  "solargraph.formatting": true,
  "solargraph.diagnostics": true,
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt"
    }
  },
  "cSpell.userWords": [
    "AMGI",
    "Authorizable",
    "Colemak",
    "Covid",
    "Dispatchable",
    "Drasner's",
    "Fireable",
    "Greetable",
    "Humi",
    "Intellij",
    "JIRA",
    "Kdenlive",
    "Laravel",
    "Lato",
    "Maatwebsite",
    "Macbook",
    "Macroable",
    "Manjaro",
    "Nand",
    "Neovim",
    "Paginator",
    "Percona",
    "Polybar",
    "Postgres",
    "Queueable",
    "Tetris",
    "Thinkpad",
    "Transifex",
    "autosaved",
    "behaviour",
    "borderless",
    "bspwm",
    "datasets",
    "filenamify",
    "jsonapi",
    "keyval",
    "monospace",
    "mycli",
    "nightfly",
    "pdfs",
    "repos",
    "sarc",
    "stateful",
    "syncable",
    "todo's",
    "todos",
    "tomontheinternet",
    "uuidv",
    "webm",
    "youtube"
  ],
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "markdownlint.config": { "default": true },
  "snippets.userSnippetsDirectory": "~/.config/nvim/snips"
}
